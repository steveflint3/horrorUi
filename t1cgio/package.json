{
  "name": "t1cg.io",
  "version": "1.0.3",
  "description": "The T1CG Company Website",
  "main": "index.js",
  "scripts": {
    "start": "TS_NODE_PROJECT=\"webpack.tsconfig.json\" webpack-dev-server --mode development --display-error-details --config ./webpack.dev.ts",
    "test": "jest --coverage",
    "build": "TS_NODE_PROJECT=\"webpack.tsconfig.json\" webpack --mode production --config ./webpack.prod.ts",
    "build:analyze": "ANALYZE=true npm run build",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint-fix": "./node_modules/.bin/tslint -c tslint.json  --fix 'src/**/*.{ts,tsx}'",
    "prettier": "./node_modules/.bin/prettier --check 'src/**/*.{ts,tsx,js}'",
    "prettier-fix": "./node_modules/.bin/prettier --check 'src/**/*.{ts,tsx,js}' --write",
    "lint-style": "./node_modules/.bin/stylelint ./src/**",
    "lint-style-fix": "./node_modules/.bin/stylelint ./src/** --fix",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lighthouse": "npm run build && node ./lighthouse.js",
    "type": "./node_modules/.bin/tsc -p ./tsconfig.json --skipLibCheck --noEmit",
    "version": "./node_modules/.bin/conventional-changelog -p angular -i ./CHANGELOG.md -r 0 -s && git add CHANGELOG.md"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "@storybook/addon-a11y": "^5.0.6",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-centered": "^5.0.6",
    "@storybook/addon-info": "^5.2.3",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@svgr/webpack": "^4.1.0",
    "@types/autoprefixer": "^9.4.0",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/highlight.js": "^9.12.3",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.11",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^11.13.9",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.8",
    "@types/react-router-dom": "^4.3.1",
    "@types/storybook__addon-knobs": "^4.0.5",
    "@types/storybook__react": "^4.0.1",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.4.29",
    "@types/webpack-bundle-analyzer": "^2.13.1",
    "@types/webpack-dev-server": "^3.1.5",
    "autoprefixer": "^9.4.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "conventional-changelog": "^3.0.5",
    "conventional-changelog-cli": "^2.0.11",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lighthouse": "^5.0.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "1.16.1",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "sass-loader": "^7.1.0",
    "storybook-react-router": "^1.0.5",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-order": "^2.0.0",
    "svg-react-loader": "^0.4.6",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.1.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "es6-promise": "^4.2.6",
    "highlight.js": "^9.13.1",
    "lottie-web": "^5.5.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.0.6",
    "react-router-dom": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run type",
      "commit-msg": "./node_modules/.bin/commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint-fix",
      "npm run prettier-fix",
      "git add"
    ],
    "*.scss": [
      "npm run lint-style-fix",
      "git add"
    ]
  }
}
